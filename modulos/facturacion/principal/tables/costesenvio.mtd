		<visiblegrid>false</visiblegrid>
	</field>
	
	<field>
		<name>codenvio</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","F. Envio")</alias>
		<null>true</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>10</length>
		<relation>
			<table>formasenvio</table>
			<field>codenvio</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
