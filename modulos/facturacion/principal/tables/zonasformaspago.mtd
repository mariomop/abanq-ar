<!DOCTYPE TMD>
<TMD>
	<name>zonasformaspago</name>
	<!-- Listado de formas de pago por zona -->
	<alias>QT_TRANSLATE_NOOP("MetaData","Formas de pago por zona")</alias>
	
	<field>
		<name>id</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
	</field>
	
	<field>
		<name>codzona</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Zona")</alias>
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>20</length>
		<relation>
			<table>zonasventa</table>
			<field>codzona</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>codpago</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","F. Pago")</alias>
		<null>true</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>10</length>
		<relation>
			<table>formaspago</table>
			<field>codpago</field>
			<card>M1</card>
		</relation>
	</field>
</TMD>
