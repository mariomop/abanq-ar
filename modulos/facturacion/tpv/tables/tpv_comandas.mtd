		<pk>false</pk>
		<type>string</type>
		<length>15</length>
		<optionslist>QT_TRANSLATE_NOOP("MetaData","Efectivo");QT_TRANSLATE_NOOP("MetaData","Cta.Cte.");QT_TRANSLATE_NOOP("MetaData","Tarjeta");QT_TRANSLATE_NOOP("MetaData","Cheque");QT_TRANSLATE_NOOP("MetaData","Vale")</optionslist>
		<default>QT_TRANSLATE_NOOP("MetaData","Efectivo")</default>
	</field>
	<field>
		<name>codpago</name>
		<!--Forma de pago-->        <alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
