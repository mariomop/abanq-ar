		<pk>false</pk>
		<type>string</type>
		<length>16</length>
		<editable>false</editable>
		<default>0</default>
		<!--        <counter>true</counter>-->
		<relation>
			<table>tpv_i_comandas</table>
			<field>h_tpv__comandas_codigo</field>
			<card>1M</card>
		</relation>
		<relation>
			<table>tpv_i_comandas</table>
			<field>d_tpv__comandas_codigo</field>
			<card>1M</card>
		</relation>
	</field>
	<field>
		<name>tipoventa</name>
		<!-- Tipo de Venta: Factura A, B, Ticket,... -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>20</length>
		<optionslist>Factura,Ticket,Remito,Pedido,Presupuesto</optionslist>
		<default>Factura</default>
	</field>
	<field>
		<name>claseventa</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Clase")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>1</length>
		<editable>false</editable>
	</field>
	<field>
		<name>estado</name>
		<!-- Estado en el que se encuentra la comanda: Cerrada(ya ha sido pagada) o Abierta -->        <alias>QT_TRANSLATE_NOOP("MetaData","Estado")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>
		<optionslist>QT_TRANSLATE_NOOP("MetaData","Cerrada");QT_TRANSLATE_NOOP("MetaData","Abierta");QT_TRANSLATE_NOOP("MetaData","Parcial")</optionslist>
		<default>QT_TRANSLATE_NOOP("MetaData","Abierta")</default>
		<editable>false</editable>
	</field>
	<field>
		<name>idtpv_arqueo</name>
		<!-- Código del arqueo al que pertenece -->        <alias>QT_TRANSLATE_NOOP("MetaData","Arqueo")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>16</length>
		<editable>false</editable>
		<visiblegrid>false</visiblegrid>
		<relation>
			<table>tpv_arqueos</table>
			<field>idtpv_arqueo</field>
			<card>M1</card>
		</relation>
	</field>
	<field>
