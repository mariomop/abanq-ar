		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		<relation>
			<table>tpv_lineascomanda</table>
			<field>idtpv_comanda</field>
			<card>1M</card>
		</relation>
		<relation>
			<table>tpv_pagoscomanda</table>
			<field>idtpv_comanda</field>
			<card>1M</card>
		</relation>
		<relation>
			<table>tpv_vales</table>
			<field>idtpv_comanda</field>
			<card>1M</card>
		</relation>
	</field>
	<field>
		<name>codigo</name>
		<!-- Código de venta-->        <alias>QT_TRANSLATE_NOOP("MetaData","Venta")</alias>
