<!DOCTYPE TMD>

<TMD>
	
	<name>i_etiquetas_stock</name>
	
	<!-- Listado de informes de articulos por stock	-->
	
	<alias>QT_TRANSLATE_NOOP("MetaData","Listado de etiquetas por stock")</alias>
	
	
	
	<field>
		
		<name>id</name>
		
		<!-- Identificador interno	-->
		
		<alias>QT_TRANSLATE_NOOP("MetaData","ID")</alias>
		
		<null>false</null>
		
		<pk>true</pk>
		
		<type>serial</type>
		
		<visiblegrid>false</visiblegrid>
		
	</field>
	
	
	
	<field>
		
		<name>descripcion</name>
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		
		<pk>false</pk>
		
		<null>false</null>
		
		<type>string</type>
		
		<length>200</length>
		
	</field>
	
	
	
	<field>
		
		<name>d_articulos_referencia</name>
		
		<!-- Código de artículo inicial -->
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Desde artículo Nº")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<length>18</length>
		
		
		
		<relation>
			
			<table>articulos</table>
			
			<field>referencia</field>
			
			<card>M1</card>
			
		</relation>
		
		
		
	</field>
	
	
	
	<field>
		
		<name>h_articulos_referencia</name>
		
		<!-- Código de artículo final -->
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Hasta artículo Nº")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<length>18</length>
		
		
		
		<relation>
			
			<table>articulos</table>
			
			<field>referencia</field>
			
			<card>M1</card>
			
		</relation>
		
		
		
	</field>
	
	
	
	<field>
		
		<name>orden1</name>
		
		<!-- Primer criterio de ordenación -->
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Familias por (1)")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<optionslist>QT_TRANSLATE_NOOP("MetaData","No ordenar");QT_TRANSLATE_NOOP("MetaData","Código");QT_TRANSLATE_NOOP("MetaData","Descripción")</optionslist>
		
		<length>20</length>
		
	</field>
	
	
	
	<field>
		
		<name>tipoorden1</name>
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Orden")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<optionslist>QT_TRANSLATE_NOOP("MetaData","Ascendente");QT_TRANSLATE_NOOP("MetaData","Descendente")</optionslist>
		
		<length>20</length>
		
	</field>
	
	
	
	<field>
		
		<name>orden2</name>
		
		<!-- Criterio de orden por artículo	-->
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Artículos por")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<optionslist>QT_TRANSLATE_NOOP("MetaData","No ordenar");QT_TRANSLATE_NOOP("MetaData","Referencia");QT_TRANSLATE_NOOP("MetaData","Descripción")</optionslist>
		
		<length>20</length>
		
	</field>
	
	
	
	<field>
		
		<name>tipoorden2</name>
		
		<alias>QT_TRANSLATE_NOOP("MetaData","Orden")</alias>
		
		<pk>false</pk>
		
		<type>string</type>
		
		<optionslist>QT_TRANSLATE_NOOP("MetaData","Ascendente");QT_TRANSLATE_NOOP("MetaData","Descendente")</optionslist>
		
		<length>20</length>
		
	</field>
	
	
	
	
</TMD>



