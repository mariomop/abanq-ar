<!DOCTYPE TMD>
<TMD>
	<name>lineastransstock</name>
	<!-- Líneas de transferencia de stock entre almacenes -->    <alias>QT_TRANSLATE_NOOP("MetaData","Líneas de transferencia de stock")</alias>
	<field>
		<name>idlinea</name>
		<!-- Identificador interno -->        <alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		<relation>
			<table>movilote</table>
			<field>idlineats</field>
			<card>1M</card>
		</relation>
	</field>
	<field>
		<name>idtrans</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Transferencia")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>
		<relation>
			<table>transstock</table>
			<field>idtrans</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	<field>
		<name>referencia</name>
		<!-- Código de referencia del artículo -->        <alias>QT_TRANSLATE_NOOP("MetaData","Artículo")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>18</length>
		<relation>
			<table>articulos</table>
			<field>referencia</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	<field>
		<name>numserie</name>
		<!-- Número de serie del artículo adquirido -->        <alias>QT_TRANSLATE_NOOP("MetaData","Nº de serie")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>25</length>
		<relation>
			<table>numerosserie</table>
			<field>numserie</field>
			<card>M1</card>
		</relation>
	</field>
	<field>
		<name>codlote</name>
		<!-- Identificador del lote asociado-->        <alias>QT_TRANSLATE_NOOP("MetaData","Id. lote")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>30</length>
		<relation>
			<field>codlote</field>
			<table>lotes</table>
			<card>M1</card>
		</relation>
		<associated>
			<with>referencia</with>
			<by>referencia</by>
		</associated>
	</field>
	<field>
		<name>descripcion</name>
		<!--Descripción del artículo-->        <alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
	</field>
	<field>
		<name>cantidad</name>
		<!-- Valor del stock -->        <alias>QT_TRANSLATE_NOOP("MetaData","Cantidad")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<partI>10</partI>
		<partD>2</partD>
		<default>0</default>
	</field>
</TMD>
