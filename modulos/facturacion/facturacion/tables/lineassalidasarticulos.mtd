<!DOCTYPE TMD>
<TMD>
	<name>lineassalidasarticulos</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Salidas de artículos")</alias>
	
	<field>
		<name>idlineasalida</name>
		<!--Identificador interno de la tabla-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
	</field>
	
	<field>
		<name>idlineafactura</name>
		<!--Identificador de la línea de factura a la que pertenece-->
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>lineasfacturascli</table>
			<field>idlinea</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	
	<field>
		<name>idfactura</name>
		<!--Identificador de la factura a la que pertenece-->
