		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>lineaspresupuestoscli</table>
			<field>idpresupuesto</field>
			<card>1M</card>
		</relation>
		<relation>
			<table>tpv_comandas</table>
			<field>iddocumento</field>
			<card>1M</card>
		</relation>
